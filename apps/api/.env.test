# Test environment
NODE_ENV=test
PORT=3001

# Database (use in-memory or test database)
DATABASE_URL="postgresql://username:password@localhost:5432/kol_analytics_test"

# Redis (use separate test instance)
REDIS_URL="redis://localhost:6379/1"

# Crawler Configuration (disabled for tests)
CRAWLER_CONCURRENT_REQUESTS=1
CRAWLER_REQUEST_DELAY=100
CRAWLER_TIMEOUT=5000

# API Configuration
API_RATE_LIMIT_WINDOW=60000
API_RATE_LIMIT_MAX=1000

# Cache TTLs (shorter for tests)
CACHE_TTL_PROFILE=60
CACHE_TTL_PRICING=30
CACHE_TTL_AUDIENCE=60
CACHE_TTL_PERFORMANCE=30
CACHE_TTL_CONVERSION=60
CACHE_TTL_MARKETING_INDEX=15