# Development environment
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL="postgresql://username:password@localhost:5432/kol_analytics_dev"

# Redis
REDIS_URL="redis://localhost:6379"

# Crawler Configuration
CRAWLER_CONCURRENT_REQUESTS=3
CRAWLER_REQUEST_DELAY=2000
CRAWLER_TIMEOUT=30000

# Optional: Add crawler credentials if available
# CRAWLER_USERNAME=
# CRAWLER_PASSWORD=
# CRAWLER_SESSION_COOKIES=

# API Configuration
API_RATE_LIMIT_WINDOW=60000
API_RATE_LIMIT_MAX=100

# Cache TTLs (in seconds)
CACHE_TTL_PROFILE=3600
CACHE_TTL_PRICING=1800
CACHE_TTL_AUDIENCE=7200
CACHE_TTL_PERFORMANCE=1800
CACHE_TTL_CONVERSION=3600
CACHE_TTL_MARKETING_INDEX=900